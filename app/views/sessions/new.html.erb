<h2>Log in</h2>
<%= form_for(:session, url: login_path) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit 'Login' %>
<% end %>

<p>New user? <%= link_to "Sign up now!", new_user_path %></p>
